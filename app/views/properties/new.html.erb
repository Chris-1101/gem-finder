<div class="conatiner">
  <h1>Enter Your Address Below:</h1>
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3">
      <%= simple_form_for(@property) do |f|%>
      <div class="col-xs-12 col-sm-4-offset-3">
        <%= f.input :address, id: "property_address" %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.submit class: "btn btn-danger" %>
      </div>
    </div>
    <% end %>
  </div>
</div>
